<h1>LogIn Here</h1>

<%= form_for :user, :url => user_path do |f| %>
  <p>
    <%= f.label :Username %><br>
    <%= f.text_field :email_id %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_field :password %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>


